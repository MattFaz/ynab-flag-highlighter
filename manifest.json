{
    "manifest_version": 3,
    "name": "YNAB Flag Highlighter",
    "version": "1.0",
    "description": "Highlight rows in your YNAB transaction list based on Flag color",
    "icons": {
        "48": "icons/logo-48.png",
        "96": "icons/logo-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://app.ynab.com/*/accounts*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}